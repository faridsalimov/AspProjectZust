        <div class="my-profile-inner-box">
            <div class="profile-cover-image">
                <a href="#">
                    <img src="~/assets/images/my-profile-bg.jpg" alt="image">
                </a>
                <a href="#" class="edit-cover-btn">Edit Cover</a>
            </div>

            <div class="profile-info-box">
                <div class="inner-info-box d-flex justify-content-between align-items-center">
                    <div class="info-image">
                        <a href="#">
                            <div style="width:250px;height:250px">
                                <img style="width:100%;height:100%;" src="~/assets/images/user/@ViewBag.User.ImageUrl" alt="image">
                            </div>
                        </a>
                        <div class="icon">
                            @* <a href="#"> *@
                            <form asp-controller="Home" asp-action="MyProfile" method="post" enctype="multipart/form-data"role="form" class="form-horizontal">
                                <input style="right: 0px;width: 110px;padding: 10px;" type="file" asp-for="@Model.File">
                                <button style="display: block;margin-left: auto;margin-right: auto;" type="submit" class="btn btn-info">Upload Image</button>
                            </form>
                            @* </a> *@
                        </div>
                    </div>
                    <div class="info-text ms-3">
                        <h3><a href="#">@ViewBag.User.UserName</a></h3>
                        <span><a href="mailto:matthew@gmail.com">@ViewBag.User.Email</a></span>
                    </div>
                    <ul class="statistics">
                        <li>
                            <a href="#">
                                <span class="item-number likeCount">@ViewBag.User.LikeCount</span>
                                <span class="item-text">Likes</span>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <span class="item-number followingCount">@ViewBag.User.FollowingCount</span>
                                <span class="item-text">Following</span>
                            </a>
                        </li>
                        <li>
